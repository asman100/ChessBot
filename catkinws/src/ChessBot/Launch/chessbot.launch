<launch>
<!-- Launch 2 Arduino boards -->
        <!-- Launch stockfishscript.py -->
    <node name="stockfish" pkg="ChessBot" type="stockfishscript.py" output="screen" />
    <node name="gantry" pkg="ChessBot" type="gantrymovement.py" output="screen" />
  <node name="R4" pkg="rosserial_python" type="serial_node.py" output="screen">
    <param name="port" value="/dev/ttyACM2" />
     
  </node>
    <!-- Launch testmovedetect.py -->
    <node name="movedetect" pkg="ChessBot" type="movedetection.py" output="screen" />
      <node name="Mega" pkg="rosserial_python" type="serial_node.py" output="screen">
    <param name="port" value="/dev/ttyACM1" />
    
  </node>
    <node name="Giga" pkg="rosserial_python" type="serial_node.py" output="screen">
    <param name="port" value="/dev/ttyACM0" />
    
  </node>




  <!-- Launch the rosserial node for the R4 device -->

  
</launch>

